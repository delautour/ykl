_name: My App
---
apiVersion: v1
kind: Deployment
metadata:
  name:= _name
spec:
  replicas: 3
  podTemplate:
    containers:
      - name: app
        image: app:1.0
      - name: sidecar
        image: istio:25
---
apiVersion: v1
kind: ServiceAccount
metadata: # the new line here starts a new block
  name:= _name
  namespace: foo
spec: {}
